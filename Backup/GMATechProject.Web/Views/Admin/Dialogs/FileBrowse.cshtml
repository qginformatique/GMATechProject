  <body class="admin">
  
  	<h4>SÃ©lectionnez un fichier</h4>
	
	<div id="fileTree" class="demo">
	</div>
  
	<script type="text/javascript" src="/Scripts/libs/jquery.js"></script>
	<script type="text/javascript" src="/Scripts/libs/jqueryFileTree.js"></script>
	
	<script type="text/javascript">
		$(document).ready(function(){			
			// Load FileTree
			$('#fileTree').fileTree({ script: '/admin/fileManager/cke/filebrowse/getTree' }, function(file) {
		        window.opener.CKEDITOR.tools.callFunction( @Model.CKEditorFuncNum, file );
		        window.close();
		    });
		});
	</script>
  
  </body>  
  
</html>